{% extends 'base.html.twig' %}
{% block body %}

<div class="row">
<div class="col-md-9">
  <h3>Текущие параметры обзвона</h3><br>
  <ul>
    <li>Название {{ dial.name }}</li>
    <li>Активность {{ dial.active }}</li>
    <li>Дата создания {{ dial.dateCreate }}</li>
    <li>Дата начала обзвона {{ dial.dateStart }}</li>
    <li>Обзвон стартовал {{ dial.dateCallStart }}</li>
    <li>Обзвон закончился {{ dial.dateCallEnd }}</li>
    <li>Комментарий {{ dial.comment }}</li>
  </ul>
</div>
</div>

<div class="row">
  <div class="col-md-10">
  <h2 class="page-header">Редактировать параметры обзвона</h2>
  {{ form_start(form) }}
  {{ form_widget(form) }}
  {{ form_end(form) }}
  </div>
</div>

{% endblock %}
{% block stylesheets %}
{% endblock %}
